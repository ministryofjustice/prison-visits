<%= content_for :analytics do %>
  visitMetadata = <%= GoogleAnalyticsAdapter.new(visit).to_json.html_safe %>;

  ga('set', {
    'dimension1': visitMetadata.prison_name,
    'dimension2': visitMetadata.slot_weekdays[0],
    'dimension3': visitMetadata.slot_times[0],
    'dimension4': visitMetadata.completed_at,
    'metric1': visitMetadata.prisoner_age,
    'metric2': visitMetadata.visitor_age,
    'metric3': visitMetadata.number_of_slots,
    'metric4': visitMetadata.number_of_adult_visitors,
    'metric5': visitMetadata.number_of_child_visitors
  });

  if (visitMetadata.days_to_first_slot) {
    ga('set', 'metric6', visitMetadata.days_to_first_slot);
  }
<% end %>
